<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!--Simple Test Suite-->
<suite name="DemoTestSuite.02">
    <test name="DemoTest.02-1">
        <classes>
            <!--All public methods with @Test annotation will run. Other public method(s) will be ignored.-->
            <class name="org.testng.training.testng.demo.test.TestB"/>
        </classes>
    </test>

    <test name="DemoTest.02-2">
        <classes>
            <!--All public methods will run as test method because @Test is used on class.-->
            <class name="org.testng.training.testng.demo.test.TestC"/>
        </classes>
    </test>
</suite>
