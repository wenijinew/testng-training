<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DemoTestSuite.04">
    <test name="DemoTest.04-1">
        <!--all @Test methods found in the package will be executed-->
        <packages>
            <!--exercise: change package name and run again-->
            <package name="org.testng.training.testng.demo.test"/>
        </packages>
    </test>
    <test name="DemoTest.04-2">
        <!--all @Test methods found in the package and subpackages will be executed-->
        <packages>
            <!--exercise: change package name regular expression and run again-->
            <package name="org.testng.training.testng.demo.test.*" />
        </packages>
    </test>
</suite>

