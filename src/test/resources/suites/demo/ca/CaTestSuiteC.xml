        <classes>
            <class name="se.ericsson.training.testng.demo.ca.CaTestC" />
        </classes>
    </test>
    <test name="DemoTest.06-2-2">
        <classes>
            <class name="se.ericsson.training.testng.demo.ca.CaTestC">
                <methods>
                    <include name="testTC2"/>
                </methods>
