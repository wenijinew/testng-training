<?xml version="1.0" encoding="UTF-8" ?>
<!--
A.2: Learn the following TestNG listener interfaces and try to implement and use them
org.testng.IConfigurationListener
org.testng.IConfigurationListener2
org.testng.IInvokedMethodListener
-->
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Solution A2 Suite">
    <listeners>
        <listener class-name="se.ericsson.training.testng.solution.ConfigListenerImpl" />
        <listener class-name="se.ericsson.training.testng.solution.InvokedMethodListenerImpl" />
    </listeners>

    <test name="Solution A2 Test">
        <classes>
            <class name="se.ericsson.training.testng.demo.test.TestA" />
            <class name="se.ericsson.training.testng.demo.ca.CaTestM" />
            <class name="se.ericsson.training.testng.demo.test.TestB" />
        </classes>
    </test>
</suite>
